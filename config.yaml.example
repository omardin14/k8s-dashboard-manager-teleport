# Kubernetes Dashboard Manager with Teleport Configuration

# Teleport Configuration
teleport:
  # Teleport Proxy endpoint (e.g., example.teleport.com:443)
  proxy_addr: "your-proxy.teleport.com:443"
  
  # Kubernetes cluster name in Teleport
  cluster_name: "your-k8s-cluster"
  
  # Teleport join token (auto-generated by 'make generate-token' or set manually)
  # Leave as placeholder - will be auto-generated if tctl is configured
  # Or generate manually via Teleport Web UI: Settings → Authentication → Tokens
  join_token: "YOUR_TELEPORT_JOIN_TOKEN_HERE"
  
  # Teleport agent namespace
  namespace: "teleport-agent"

# Kubernetes Configuration
kubernetes:
  # Dashboard namespace
  namespace: "kubernetes-dashboard"

# Docker Configuration (if building custom images)
docker:
  username: "your-dockerhub-username"

